{
  "name": "@totea/mongone",
  "version": "1.1.0",
  "description": "inject mongone & make RESTful APIs for totea",
  "author": "aimleo",
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "engines": {
    "node": ">=10.x",
    "npm": ">= 6.x"
  },
  "browserslist": [
    "maintained node versions"
  ],
  "scripts": {
    "cm": "npx git-cz",
    "release": "standard-version && npm run push && npm run pub2npm",
    "release:d": "standard-version --dry-run",
    "push": "git push --follow-tags origin master",
    "pub2npm": "nrm use npm && npm publish",
    "lint": "node_modules/eslint/bin/eslint.js ./*.js --fix"
  },
  "dependencies": {
    "@mongone/encodeuri": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "tegund": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "standard-version": "^9.1.1"
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "extends": "standard"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aim-leo/totea-mongone.git"
  },
  "bugs": {
    "url": "https://github.com/aim-leo/totea-mongone/issues"
  },
  "homepage": "https://github.com/aim-leo/totea-mongone#readme"
}
